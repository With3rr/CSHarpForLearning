using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp39
{
    class Program
    {
        static void Main(string[] args)
        {
            //kolejka
            Queue<int> queue = new Queue<int>();
            queue.Enqueue(1);
            queue.Enqueue(2);
            queue.Enqueue(3);
            Console.WriteLine(queue.Dequeue()); //usuwanie i wypisywanie element√≥w FiFo
            Console.WriteLine(queue.Dequeue());
            Console.WriteLine(queue.Dequeue());
            //stos

            Stack<int>st = new Stack<int>();
            st.Push(1);
            st.Push(2);
            st.Push(3);
            Console.WriteLine(st.Pop()); //LIFO usuwanie z stosu
            Console.WriteLine(st.Pop());
            Console.WriteLine(st.Pop());

            Console.ReadLine();
        }
    }
}
